{
  "name": "eslint-plugin-avoid-jest-spy-on",
  "version": "0.0.1",
  "description": "eslint plugin to avoid jest.spyOn",
  "main": "src/index.js",
  "repository": "git@github.com:bailey-byb/avoid-jest-spyOn.git",
  "author": "Bailey Sheather <bailey@beforeyoubid.com.au>",
  "license": "GPL-2.0-or-later",
  "files": [
    "dist/src/**",
    "!dist/src/**/__types/**"
  ],
  "scripts": {
    "build": "rimraf dist && mkdir -p dist && tsc -p tsconfig.json --outDir dist && cp package.json dist/",
    "test": "jest --coverage",
    "lint": "eslint **/*.ts",
    "ts": "tsc --noEmit",
    "build-and-publish": "npm run build && npm publish --access public",
    "build-and-publish-dryrun": "npm run build && npm publish --dry-run",
    "build-and-publish-dryrun-ci": "npm run build && npm publish --registry http://localhost:4873"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^5.51.0"
  },
  "devDependencies": {
    "@swc/core": "^1.3.35",
    "@swc/jest": "^0.2.24",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "danger": "^11.2.3",
    "danger-plugin-yarn": "^1.6.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.2",
    "jest-junit": "^15.0.0",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "typescript": "^4.9.5"
  }
}
